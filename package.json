{
  "name": "qbmi",
  "version": "0.0.20",
  "description": "组件库",
  "keywords": [
    "组件"
  ],
  "homepage": "https://gitee.com/qbmi/qbmi",
  "bugs": {
    "url": "https://gitee.com/qbmi/qbmi/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://gitee.com/qbmi/qbmi.git"
  },
  "license": "MIT",
  "files": [
    "dist",
    "lib",
    "es"
  ],
  "sideEffects": [
    "dist/*",
    "es/**/style/*",
    "lib/**/style/*",
    "*.less"
  ],
  "main": "lib/index.js",
  "module": "es/index.js",
  "unpkg": "dist/index.js",
  "typings": "lib/index.d.ts",
  "author": "hh",
  "contributors": [
    "ant"
  ],
  "scripts": {
    "start": "dumi dev",
    "build": "father-build",
    "docs:build": "dumi build",
    "docs:deploy": "gh-pages -d docs-dist",
    "deploy": "npm run docs:build && npm run docs:deploy",
    "release": "npm run build && npm publish",
    "analyze": "cross-env ANALYZE=1 dumi build",
    "prettier": "prettier --write \"**/*.{js,jsx,tsx,ts,less,md,json}\"",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@jiaminghi/data-view-react": "^1.2.3",
    "@umijs/plugin-esbuild": "^1.0.1",
    "cross-env": "^7.0.2",
    "echarts": "^4.8.0",
    "lodash": "^4.17.20",
    "pdfjs-dist": "2.4.456",
    "qs": "^6.9.3"
  },
  "devDependencies": {
    "@types/pdfjs-dist": "^2.1.5",
    "@umijs/preset-react": "1.x",
    "@umijs/test": "^3.0.5",
    "dumi": "^1.0.9",
    "father-build": "^1.17.2",
    "gh-pages": "^3.0.0",
    "lint-staged": "^10.0.7",
    "prettier": "^1.19.1",
    "yorkie": "^2.0.0"
  },
  "browserslist": [
    "last 2 versions",
    "Firefox ESR",
    "> 1%",
    "ie >= 11"
  ],
  "peerDependencies": {
    "antd": "^4",
    "react": ">=16.12.0",
    "react-dom": ">=16.9.0"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  }
}
